{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "jquery": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": ["off"],
    "no-unused-vars": ["error", { "args": "none" }],
    "operator-linebreak": ["error", "after"],
    "import/prefer-default-export": 0,
    "no-await-in-loop": ["off"]
  }
}
